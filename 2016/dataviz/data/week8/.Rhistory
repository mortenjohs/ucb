group_by(region, year) %>%
summarize(sum_gdp = sum(gdp_tn))
View(regions_gdp)
regions_gdp <- nations %>%
group_by(region, year) %>%
summarize(sum_gdp = sum(gdp_tn, na.rm=TRUE))
View(regions_gdp)
regions_gdp <- nations %>%
